function setup(){
}

function draw()
  {
    createCanvas(w = 800, w, WEBGL);
    background(255);
    colorMode(HSB, 360, 100, 100, 100);
      for  (let i = 30; i < w; i +=30){
        rotateZ(frameCount * .001);
        translate(0,2);    
        rotateY(PI * w/2),
        noFill();
        stroke (i%360, 100, 100, 100);
        (box(200));
}}
